# Frontend Environment Variables
# Copy this file to .env.local or .env and add your actual keys

# API Configuration
REACT_APP_API_BASE_URL=http://localhost:9090

# Google OAuth
# TODO: Get from Google Cloud Console (https://console.cloud.google.com/)
# Steps:
# 1. Create project in Google Cloud Console
# 2. Enable Google+ API
# 3. Create OAuth 2.0 credentials (Web application)
# 4. Add http://localhost:3000 to authorized origins
# 5. Copy Client ID here
REACT_APP_GOOGLE_CLIENT_ID=YOUR_GOOGLE_CLIENT_ID_HERE

# Feature Flags
REACT_APP_ENABLE_GOOGLE_AUTH=true
REACT_APP_ENABLE_EMAIL_AUTH=true
REACT_APP_ENABLE_GUEST_AUTH=true
